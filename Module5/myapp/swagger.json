{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Calculator API",
        "description": "API documentation for the Calculator application"
    },
    "host": "localhost:3000",
    "basePath": "/",
    "schemes": [
        "http"
    ],
    "paths": {
        "/calculator/add": {
            "get": {
                "summary": "Add two numbers",
                "description": "Returns the sum of two numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "required": true,
                        "type": "integer",
                        "description": "The first number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "required": true,
                        "type": "integer",
                        "description": "The second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/calculator/subtract": {
            "get": {
                "summary": "Subtract two numbers",
                "description": "Returns the difference of two numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "required": true,
                        "type": "integer",
                        "description": "The first number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "required": true,
                        "type": "integer",
                        "description": "The second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/calculator/multiply": {
            "get": {
                "summary": "Multiply two numbers",
                "description": "Returns the product of two numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "required": true,
                        "type": "integer",
                        "description": "The first number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "required": true,
                        "type": "integer",
                        "description": "The second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/calculator/divide": {
            "get": {
                "summary": "Divide two numbers",
                "description": "Returns the quotient of two numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "required": true,
                        "type": "integer",
                        "description": "The first number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "required": true,
                        "type": "integer",
                        "description": "The second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "integer"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Division by zero is not allowed",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}